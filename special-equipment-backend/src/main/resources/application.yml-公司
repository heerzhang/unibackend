#数据库连接，屏蔽的可直接加序号改名字==弃用。
#数据库连接用户schema, 不管底层物理数据库是否集群分布还是云啥的，这里实际是单用户可见的数据库。
app:
  datasource:
    sei:
      url: jdbc:oracle:thin:@//192.168.104.34:1521/fjtj
      username: fjseiinuser
      password: fjseiuser12345678
    #源程序中@ConfigurationProperties(prefix="app.datasource.sei")和这里名字要一致。
    #若没有Oracle可用，本机可使用H2数据库测试就行。　可能要修改Hibernate中配置数据库方言设置。
    sei6:
      url: jdbc:h2:tcp://localhost/~/sei2
      username: sa
      password:
    #监察等其它的数据库。每一个用户数据库都独立配置。JPA一次Query无法跨越多个数据库,当然上层的逻辑代码可以支持任意数量数据库。
    incp4:
      url: jdbc:h2:tcp://localhost/~/incp
      username: sa
      password:
    #若没有开启本机H2的console，将会自动使用H2内存数据库。
    sdn4:
      url: jdbc:h2:tcp://localhost/~/sdn
      username: sa
      password:
    #旧平台正式运行的。
    fjtj:
      url: jdbc:oracle:thin:@//192.168.168.6:1521/fjtj
      username: fjseiinuser
      password: fjseiuser12345678


# spring
spring:
  servlet:
    multipart:
      enabled: true
      file-size-threshold: 2KB
      max-file-size: 200MB
      max-request-size: 215MB
  application:
    name: fjtj
  profiles:
    active:
    - product

  #jta相关参数配置
  jta:
    atomikos:
      properties:
        #没效果? 要去jta.properties文件
        #default-jta-timeout: 2600000
        #max-timeout: 30000000
        enable-logging: true
  #log-dir: classpath:newlog
  #transaction-manager-id: txManager
  jpa:
    properties:
      javax:
        persistence:
          sharedCache.mode: ENABLE_SELECTIVE
      #开启查询缓存factory_class    cache.use_query_cache: true
      hibernate.format_sql: true
      hibernate.use_sql_comments: true
      hibernate:
        #default_schema: newsei   还是一个个指定好
        #看SQL打印　true 　false
        show_sql: true
        #统计对单个hibernate session会话的总结
        generate_statistics: true
        ##      dialect: org.hibernate.dialect.Oracle10gDialect
        hbm2ddl:
          auto: update
        #指定缓存provider
        cache:
          region.factory_class: org.hibernate.cache.ehcache.SingletonEhCacheRegionFactory
          #二级缓存关闭启用的2个总开关,关不了集合字段
          use_second_level_cache: true
          use_query_cache: true
    generate-ddl: true
    open-in-view: true

  main:
    allow-bean-definition-overriding: true
  mvc:
    view:
      prefix: /
      #suffix: .jsp
  cache:


graphql:
  servlet:
    mapping: /graphql
    enabled: true
    corsEnabled: true
    exception-handlers-enabled: true
  tools:
    #按resources子目录来管理模型加载的graphqls目录不一样，接口就不一样了;
    schema-location-pattern: "graphql/**/*.graphqls"
    introspection-enabled: true

graphiql:
  mapping: /graphiql
  endpoint:
    graphql: /graphql
    subscriptions: /forbidden
  static:
    basePath: /
    #生产系统graphiql可关闭
  enabled: true
  pageTitle: GraphiQL
  cdn:
    enabled: false
    version: 0.11.11
  props:
    resources:
      defaultQuery: defaultQuery.graphql
    variables:
      editorTheme: "solarized light"
  headers:
  #用这会必然带上Bearer　，但是时间固定,超时,若走cockie模式就不会,而两token也不同的。给mvc浏览器读的。
