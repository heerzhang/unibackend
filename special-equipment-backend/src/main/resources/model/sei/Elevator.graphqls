type Elevator implements Equipment & IfElevator{
    id:ID!
     cod: String!
     oid: String
     type: String!
     sort: String!
     vart: String!

     liftHeight: String

}

interface IfElevator{
    liftHeight: String
}

extend type Query {

    findAllEQPsFilter(where: DeviceCommonInput, offset:Int, first:Int, orderBy:String, asc:Boolean): [Equipment]!
}

extend type Mutation {
    buildElevator(cod: String!, type: String!, oid: String) : Elevator

}

