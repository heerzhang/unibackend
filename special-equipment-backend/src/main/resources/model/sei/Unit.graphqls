type Unit{
    id:ID!
    name: String!
    address: String
    linkMen: String
    phone: String
    owns: [EQP]
    maints: [EQP]
}
type UnitEs{
    id:ID!
    name: String!
    address: String
    linkMen: String
    phone: String
}

extend type Query {
    findAllUnits: [Unit]!
    findUnitbyName(name: String!): [UnitEs]!
    findUnitbyNameCompl(name: String!): [String]!
}

extend type Mutation {
    newUnit(name: String!, address: String) : Unit!
}

